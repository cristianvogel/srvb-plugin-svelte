{
  "name": "ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "clean": "rimraf dist && rimraf sveltekit/build && rimraf native/build",
    "predev": "npm run clean",
    "prebuild": "npm run clean",
    "dev": "npm run dev-native && concurrently \"npm run dev-dsp\" \"vite\"",
    "dev-native": "zx scripts/build-native.mjs --dev",
    "dev-dsp": "esbuild dsp/main.js --watch --bundle --outfile=public/dsp.main.js",
    "build-native": "zx scripts/build-native.mjs",
    "build-dsp": "esbuild dsp/main.js --bundle --outfile=public/dsp.main.js",
    "build-ui": "vite build",
    "dev-ui": "vite dev",
    "build": "npm run build-ui && npm run build-dsp && npm run build-native",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@elemaudio/core": "^3.0.0",
    "cpy-cli": "^4.2.0",
    "esbuild": "^0.17.8",
    "invariant": "^2.2.4",
    "resize-observer-polyfill": "^1.5.1",
    "rimraf": "^5.0.0",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@skeletonlabs/skeleton": "^2.0.0",
    "@skeletonlabs/tw-plugin": "^0.1.0",
    "@sveltejs/adapter-auto": "^3.1.1",
    "@sveltejs/adapter-static": "^3.0.1",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/eslint": "8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.36.0-next.4",
    "postcss": "^8.4.24",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "svelte": "^4.0.0",
    "svelte-check": "^3.6.0",
    "tailwindcss": "^3.3.2",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.3",
    "vite-plugin-tailwind-purgecss": "0.2.0",
    "zx": "^7.2.3"
  }
}
